<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Number_Must_Be_6_Digits</fullName>
    <active>true</active>
    <errorConditionFormula>LEN(AccountNumber) != 6</errorConditionFormula>
    <errorDisplayField>AccountNumber</errorDisplayField>
    <errorMessage>Account Number must be 6 digits</errorMessage>
</ValidationRule>
